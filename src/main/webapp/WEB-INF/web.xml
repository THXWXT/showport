<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>NewServlet</servlet-name>
        <servlet-class>com.cs.ciscloud.NewServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Servlettest</servlet-name>
        <servlet-class>com.cs.ciscloud.Servlettest</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.cs.ciscloud.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>getJWTToken</servlet-name>
        <servlet-class>com.cs.ciscloud.getJWTToken</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddUser</servlet-name>
        <servlet-class>com.cs.ciscloud.AddUser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logoutServlet</servlet-name>
        <servlet-class>com.cs.ciscloud.logoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ShowPort</servlet-name>
        <servlet-class>com.cs.ciscloud.ShowPort</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ToShowPortList</servlet-name>
        <servlet-class>com.cs.ciscloud.entity.ToShowPortList</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NewServlet</servlet-name>
        <url-pattern>/NewServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Servlettest</servlet-name>
        <url-pattern>/Servlettest</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>getJWTToken</servlet-name>
        <url-pattern>/getJWTToken</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddUser</servlet-name>
        <url-pattern>/AddUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logoutServlet</servlet-name>
        <url-pattern>/logoutServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ShowPort</servlet-name>
        <url-pattern>/ShowPort</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ToShowPortList</servlet-name>
        <url-pattern>/ToShowPortList</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
